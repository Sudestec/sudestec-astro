---
import Layout from '../layouts/Layout.astro';
---

<Layout title='Servicios'>
  <h1>Register</h1>
  <form method="POST">
    <label>
      Username:
      <input type="text" name="username" />
    </label>
    <label>
      Email:
      <input type="email" name="email" />
    </label>
    <label>
      Password:
      <input type="password" name="password" />
    </label>
    <button>Submit</button>
  </form></Layout>
